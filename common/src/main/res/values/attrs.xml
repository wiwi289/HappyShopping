<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MyTitleBar">
        <attr name="imgleft" format="integer" />
        <attr name="imgright" format="integer" />
        <attr name="texttitle" format="string" />
        <attr name="textright" format="string" />
        <attr name="is_head_back" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SecondsJumpBtn">
        <attr name="text_color" format="color|reference"/>
        <attr name="bg_color" format="color|reference"/>
        <attr name="text_size" format="integer"/>
        <attr name="seconds" format="integer"/>
    </declare-styleable>

</resources>